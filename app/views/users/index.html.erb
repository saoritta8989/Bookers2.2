	<%= render 'books/list', book: @book %>

	<div class="col-md-9">

<h1>Users</h1>

<table class="table table-striped">
	<thead>
		<tr>
			<th>image</th>
			<th>name</th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
		<tr>
			<td> <%= attachment_image_tag user, :profile_image, class: "user_image", fallback: "t2AdqcaA.jpeg", :size =>'30x30' %></td>
			<td><%= user.username%></td>
			<td><%= link_to 'Show' , user_path(user.id) %></td>
		</tr>
		<% end %>
	</tbody>
</table>
</div>
